        [NtCoreLib.Net.Firewall.FirewallMatchType]$MatchType = "Equal"
        $interfaceTypeGUID = [NtCoreLib.Net.Firewall.FirewallConditionGuids]::FWPM_CONDITION_LOCAL_INTERFACE_TYPE
